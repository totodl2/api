    "lint:eslint": "NODE_ENV=production eslint --ext js --ext ts --ignore-path .gitignore",
    "lint:staged": "lint-staged",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch:ts\" \"npm run dev:server\"",
    "dev:server": "node node_modules/.bin/nodemon --delay 0.05 ./dist/run.js server",
    "dev:worker:torrents": "node node_modules/.bin/nodemon --delay 0.05 ./dist/run.js torrentsWorker",
    "dev:worker:files": "node node_modules/.bin/nodemon --delay 0.05 ./dist/run.js filesWorker",
    "dev:worker:metadata": "node node_modules/.bin/nodemon --delay 0.05 ./dist/run.js metadataWorker",
    "dev:worker:hosts": "node node_modules/.bin/nodemon --delay 0.05 ./dist/run.js hostsMonitorWorker",
    "watch:ts": "tsc -w"
  },
  "lint-staged": {
    "*.js": "npm run-script lint:eslint"
  },
  "pre-commit": [
    "lint:staged"
  ],
